<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.admin.oes.About">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Developers:-"
        android:textSize="30dp"
        android:textStyle="bold"/>
    <ImageView
        android:layout_width="141dp"
        android:layout_height="102dp"
        android:src="@mipmap/navya"
        android:layout_gravity="center"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kalli Navya Sri"
        android:layout_gravity="center"/>

    <ImageView
        android:layout_width="141dp"
        android:layout_height="102dp"
        android:src="@mipmap/gayatri"
        android:layout_gravity="center"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jayavarapu Sai Gayatri"
        android:layout_gravity="center"/>
    <ImageView
        android:layout_width="141dp"
        android:layout_height="102dp"
        android:src="@mipmap/sai"
        android:layout_gravity="center"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kanneganti Sai Priya"
        android:layout_gravity="center"/>
    <ImageView
    android:layout_width="141dp"
    android:layout_height="102dp"
    android:src="@mipmap/greeshma"
    android:layout_gravity="center"
    />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jammula Greeshma Sai"
        android:layout_gravity="center"/>

</LinearLayout>
